	&lt;style&gt;
	#wheel {
		color: #FFFFFF;
		position:fixed;
		top:0px;
		left:0px;
		font-size:18px;
		list-style: none;
		margin:0px 0px 0px -20px;
		padding:0px;
	}
	#wheel li{
		background-color: %lite_background_color;
		border-radius: 30px;
		width: 30px;
		height: 30px;
		line-height: 30px;
		text-align: center;
	}
	&lt;/style&gt;
&lt;body&gt;
&lt;ul id="wheel"&gt;&lt;/ul&gt;
&lt;script&gt;
	var circleRadius = %lite_circle_radius;
	var wheel = $("#wheel");
	wheel.width(circleRadius);
	wheel.height(circleRadius);

	var confs = [%lite_confs];
	
	var angleLite = 90 / (confs.length + 1);
	for (var i = 0; i &lt; confs.length; i++) {
		var conf = confs[i];
		var curAngle = angleLite * (i + 1);
		var xLen = Math.sin(curAngle * Math.PI / 180) * circleRadius
		var yLen = Math.cos(curAngle * Math.PI / 180) * circleRadius
		var leftPoint = Math.round(xLen);
		var topPoint = circleRadius - Math.round(yLen);
		console.log(leftPoint + " " + topPoint);
		var li = "&lt;li id='item" 
				+ i + "' onclick='execTask(\"" 
				+ conf.task + "\")' style='position: absolute; left: 0px; top: 120px;";
		if (conf.fontSize) {
			li += "font-size:" + conf.fontSize + "px;"
		}
		li += "'&gt;" + conf.name + "&lt;/li&gt;"
		wheel.append(li);
		$("#item" + i).animate({left: leftPoint + 'px', top: topPoint + 'px'}, 100);
	}
	
	function execTask(task) {
		console.log(task);
    performTask(task, 5);
		performTask("lite-selector-exit", 5);
	}
&lt;/script&gt;
&lt;/body&gt;</Str>
