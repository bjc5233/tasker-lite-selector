&lt;style&gt;
#wheel {
	color: #FFFFFF;
	position:fixed;
	top:0px;
	left:0px;
	font-size:18px;
	list-style: none;
	margin:0px 0px 0px -15px;
	padding:0px;
}
#wheel li{
	background-color: %lite_background_color;
	border-radius: 30px;
	width: 30px;
	height: 30px;
	line-height: 30px;
	text-align: center;
}

#wheel li img{
	width: 20px;
	height: 20px;
	padding-top: 5px;
}
&lt;/style&gt;
&lt;body&gt;
&lt;ul id="wheel"&gt;&lt;/ul&gt;
&lt;script&gt;
var wheelLen = %lite_circle_radius;
var confs = [%lite_confs];
var circleStartXPoint = 16;
var circleStartYPoint = wheelLen - 17;
var wheel = $("#wheel");
wheel.width(wheelLen);
wheel.height(wheelLen);
if (true) {
	var li = "&lt;li id='itemX' onclick='execTask()' style='position: absolute; left: " 
			+ circleStartXPoint + "px; top: " 
			+ circleStartYPoint + "px;'&gt;X&lt;/li&gt;";
	wheel.append(li);
}


var itemId = 0;
for (var i = 0; i &lt; confs.length; i++) {
	var conf = confs[i];
	appendCircleItems(conf.circles, conf.radius);
}
function appendCircleItems(circles, radius) {
	var angleLite = 90 / (circles.length + 1);
	for (var i = 0; i &lt; circles.length; i++) {
		var circle = circles[i];
		var curAngle = angleLite * (i + 1);
		var xLen = Math.sin(curAngle * Math.PI / 180) * radius
		var yLen = Math.cos(curAngle * Math.PI / 180) * radius
		var leftPoint = Math.round(xLen);
		var topPoint = wheelLen - Math.round(yLen);
		console.log(leftPoint + " " + topPoint);
		
		
		var li = document.createElement("li");
		li.id = "item" + itemId;
		if (circle.param) {
			li.setAttribute("onclick", "execTask('" + circle.task + "', '" + circle.param + "')");
		} else {
			li.setAttribute("onclick", "execTask('" + circle.task + "')");
		}
		
		var style = "position: absolute; left: " 
					+ circleStartXPoint + "px; top: " 
					+ circleStartYPoint + "px;";
		if (circle.style) {
			style += circle.style;
		}
		li.setAttribute("style", style);
		
		if (circle.type == "img") {
			li.innerHTML = "&lt;img src='" + circle.imgPath + "'/&gt;"
		} else if (circle.type == "text") {
			li.innerText = circle.text;
		}
		wheel.append(li);
		$("#item" + itemId).animate({left: leftPoint + 'px', top: topPoint + 'px'}, 100);
		itemId ++;
	}
}


function execTask(task, param) {
	console.log(task);
	if (task) {
		performTask(task, 5, param);
	}
	performTask("lite-selector-exit", 5);
}
&lt;/script&gt;
&lt;/body&gt;</Str>
